						"customer derive({FULL NAME} = FIRST_NAME+ \" \"+LAST_NAME) ~> derivedColumn1",
						"derivedColumn1, order join(customer@CUSTOMER_ID == order@CUSTOMER_ID,",
						"     joinType:'inner',",
